{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./test",
          "includes": ["\\.spec\\.js"]
        },
        "accessor": {
          "access": ["public"]
        }
      }
    }
  ],
  "includes": ["\\.js$"]
}
