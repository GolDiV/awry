{
  "name": "awry",
  "version": "0.2.0",
  "description": "An ARI websocket and API client library",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "cover": "nyc --reporter=html --reporter=text _mocha && open coverage/index.html",
    "ci-test": "npm run lint && npm run ci-mocha && npm run ci-upload-coverage",
    "ci-mocha": "nyc mocha",
    "ci-upload-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "keywords": [
    "asterisk",
    "ari"
  ],
  "license": "MIT",
  "engines": {
    "node": "^6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadxz/awry.git"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "request": "^2.75.0",
    "request-promise-native": "^1.0.3",
    "retry": "^0.10.0",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "codecov": "^2.3.1",
    "env-test": "^1.0.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.0.2",
    "nock": "^9.0.0",
    "nyc": "^11.2.1",
    "power-assert": "^1.4.1"
  }
}
